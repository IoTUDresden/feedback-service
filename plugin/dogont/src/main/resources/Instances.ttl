@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://vicci.eu/speeront/instances>
  rdf:type owl:Ontology ;
  owl:imports <http://openhab-semantic/0.1/instance> ;
  owl:imports <http://vicci.eu/speeront> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:SuperPeer_1
  rdf:type speeront:SuperPeer ;
  dogont:isIn <http://openhab-semantic/0.1/instance#Lobby_Mueller> ;
  speeront:hasDevices <http://openhab-semantic/0.1/instance#Thing_TinkerforgeLcd> ;
  speeront:hasPeer :Peer_1 ;
  speeront:hasPeer :Peer_2 ;
  speeront:hasProcess :Process1 ;
  speeront:hasURI "localhost"^^xsd:string ;
.
:Peer_1
  rdf:type speeront:Peer ;
  dogont:IPAddress "192.168.1.63" ;
  dogont:hasState :Peer_1HeartbeatTimeState;
  dogont:hasState :Peer_1BatteryLevelState ;
  dogont:hasState :Peer_1ResponseTimeState ;
  dogont:hasState :Peer_1ExecutionTimeState;
  dogont:isIn <http://openhab-semantic/0.1/instance#LivingRoom_Mueller> ;
  speeront:hasDevices <http://openhab-semantic/0.1/instance#Thing_HomematicDimmer_1> ;
  speeront:hasDevices <http://openhab-semantic/0.1/instance#Thing_TinkerforgeAmbientLight_2> ;
  speeront:hasProcess :SubProcess1 ;
.
:Peer_2
  rdf:type speeront:Peer ;
  dogont:IPAddress "192.168.1.69" ;
  dogont:hasState :Peer_2HeartbeatTimeState ;
  dogont:hasState :Peer_2BatteryLevelState ;
  dogont:hasState :Peer_2ResponseTimeState ;
  dogont:hasState :Peer_2ExecutionTimeState;
  dogont:isIn <http://openhab-semantic/0.1/instance#LivingRoom_Mueller> ;
  speeront:hasDevices <http://openhab-semantic/0.1/instance#Thing_HomematicDimmer_1> ;
  speeront:hasDevices <http://openhab-semantic/0.1/instance#Thing_TinkerforgeAmbientLight_2> ;
.
:Peer_1HeartbeatTimeState
  rdf:type speeront:HeartbeatTimeState ;
  dogont:hasStateValue [
        rdf:type speeront:TimeValue ;
        dogont:realStateValue "0.1"^^xsd:string ;
        dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
    ] ;
.
:Peer_1ResponseTimeState
  rdf:type speeront:ResponseTimeState ;
  dogont:hasStateValue [
        rdf:type speeront:TimeValue ;
        dogont:realStateValue "0.1"^^xsd:string ;
        dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
    ] ;
.
:Peer_1ExecutionTimeState
  rdf:type speeront:ExecutionTimeState ;
  dogont:hasStateValue [
        rdf:type speeront:TimeValue ;
        dogont:realStateValue "0.1"^^xsd:string ;
        dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
    ] ;
.
:Peer_2HeartbeatTimeState
  rdf:type speeront:HeartbeatTimeState ;
  dogont:hasStateValue [
          rdf:type speeront:TimeValue ;
          dogont:realStateValue "0.1"^^xsd:string ;
          dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
      ] ;
.
:Peer_2ResponseTimeState
  rdf:type speeront:ResponseTimeState ;
  dogont:hasStateValue [
        rdf:type speeront:TimeValue ;
        dogont:realStateValue "0.1"^^xsd:string ;
        dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
    ] ;
.
:Peer_2ExecutionTimeState
  rdf:type speeront:ExecutionTimeState ;
  dogont:hasStateValue [
        rdf:type speeront:TimeValue ;
        dogont:realStateValue "0.1"^^xsd:string ;
        dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/time/second> ;
    ] ;
