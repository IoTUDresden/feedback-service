### Battery States
:Peer_1BatteryLevelState
  rdf:type speeront:BatteryLevelState ;
  dogont:hasStateValue :RobotBatteryValue ;
.
:Peer_2BatteryLevelState
  rdf:type speeront:BatteryLevelState ;
  dogont:hasStateValue :LaptopBatteryValue ;
.
:LaptopBatteryValue
  rdf:type dogont:LevelStateValue ;
  dogont:realStateValue "0.9"^^xsd:string ;
  dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/fraction/percent> ;
.
:RobotBatteryValue
  rdf:type dogont:LevelStateValue ;
  dogont:realStateValue "0.7"^^xsd:string ;
  dogont:unitOfMeasure <http://purl.oclc.org/NET/muo/ucum/unit/fraction/percent> ;
.
### Process States/Metrics
:Process1
  rdf:type speeront:Process ;
  dogont:hasState :Process1ProcessState ;
.
:SubProcess1
  rdf:type speeront:Process ;
  dogont:hasState :SubProcess1ProcessState;
.
:Process1ProcessState
  rdf:type dogont:State ;
  dogont:hasStateValue [
      rdf:type dogont:StateValue ;
      dogont:statevalueOf :ProcessState_1 ;
    ] ;
.
:SubProcess1ProcessState
  rdf:type speeront:ProcessState ;
  dogont:hasStateValue [
      rdf:type dogont:DiscreteValue ;
      dogont:statevalueOf :UndeployedStateValue_1 ;
    ] ;
.
